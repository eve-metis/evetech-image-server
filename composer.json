{
    "name": "eve-metis/evetech-image-server",
    "description": "A framework-agnostic PHP 8.3+ client for the EVE Online Image Server.",
    "type": "library",
    "keywords": [
        "eve",
        "eve-online",
        "esi",
        "image-server",
        "evetech"
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.0",
        "rector/rector": "^2.0",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EveMetis\\EveImageServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EveMetis\\EveImageServer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyse",
        "phpcs": "vendor/bin/phpcs",
        "phpcbf": "vendor/bin/phpcbf",
        "rector": "vendor/bin/rector process"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
